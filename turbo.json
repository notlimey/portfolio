{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": [".next/**", "!.next/cache/**"],
			"env": [
				"NEXT_PUBLIC_SANITY_PROJECT_ID",
				"NEXT_PUBLIC_SANITY_DATASET",
				"RESEND_API_KEY"
			]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"lint": {
			"cache": true,
			"dependsOn": ["^lint"],
			"outputs": []
		},
		"fix": {
			"cache": false,
			"dependsOn": [],
			"outputs": []
		},
		"lint-staged": {
			"cache": false,
			"dependsOn": [],
			"outputs": []
		},
		"format": {
			"cache": false
		}
	}
}
