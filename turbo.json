{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
	  "dev": {
		  "cache": false,
		  "persistent": true
	  },
	  "lint": {
		  "cache": true,
		  "dependsOn": ["^lint"],
		  "outputs": []
	  },
	  "fix": {
		  "cache": false,
		  "dependsOn": [],
		  "outputs": []
	  },
	  "lint-staged": {
		  "cache": false,
		  "dependsOn": [],
		  "outputs": []
	  },
	  "format": {
		  "cache": false
	  }
  }
}
